<VBox prefHeight="881.0" prefWidth="1139.0" styleClass="body" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.base.VisualizationController">
  <children>
    <MenuBar styleClass="top-bar" VBox.vgrow="NEVER">
      <menus>
        <Menu id="menu1" mnemonicParsing="false" text="Settings" />
        <Menu id="menu2" mnemonicParsing="false" text="About" />
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="850.0" prefWidth="1139.0" styleClass="main" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="heading" alignment="CENTER" contentDisplay="CENTER" layoutX="227.0" layoutY="14.0" prefHeight="22.0" prefWidth="685.0" textAlignment="CENTER" />
            <Line endX="44.0" endY="383.0" layoutX="29.0" layoutY="-313.0" startX="1036.0" startY="383.0" styleClass="divider" />
            <Pane fx:id="algoPane" layoutX="73.0" layoutY="77.0" prefHeight="527.0" prefWidth="993.0">
               <children>
                  <LineChart fx:id="chart" prefHeight="527.0" prefWidth="993.0" stylesheets="@../css/main.css">
                     <xAxis>
                        <NumberAxis label="Generations" side="BOTTOM" fx:id="xAxis" />
                     </xAxis>
                     <yAxis>
                        <NumberAxis fx:id="yAxis" label="Fitness" lowerBound="1000.0" minorTickCount="8" side="LEFT" tickUnit="0.1" upperBound="1.0" />
                     </yAxis>
                  </LineChart>
               </children></Pane>
            <Button layoutX="1042.0" layoutY="25.0" mnemonicParsing="false" onAction="#goBack" prefHeight="34.0" prefWidth="65.0" styleClass="button-black" text="Back" textAlignment="CENTER" textFill="WHITE" wrapText="true" />
            <Button fx:id="btnAdd" layoutX="37.0" layoutY="25.0" mnemonicParsing="false" onAction="#addDataset" prefHeight="34.0" prefWidth="146.0" styleClass="button-black" text="Add dataset" textAlignment="CENTER" textFill="WHITE" wrapText="true" />
            <ListView fx:id="listView" layoutX="138.0" layoutY="604.0" prefHeight="200.0" prefWidth="918.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
